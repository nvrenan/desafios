<Window x:Class="DesafiosWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesafiosWpf"
        mc:Ignorable="d"
        Title="Ludwig" Height="350" Width="525" WindowStyle="ToolWindow">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Height="26" Margin="32,10,0,0" TextWrapping="Wrap" Text="Desafios de Algoritimos" VerticalAlignment="Top" Width="385" TextAlignment="Center" FontSize="14.667" FontWeight="Bold"/>
        <RadioButton x:Name="rbCaixaEletronico" Content="Caixa Eletrônico" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,52,0,0" IsChecked="True"/>
        <Button x:Name="btExecute" Content="Executar" HorizontalAlignment="Left" Margin="399,274,0,0" VerticalAlignment="Top" Width="75" Click="btExecute_Click"/>
    </Grid>
</Window>
